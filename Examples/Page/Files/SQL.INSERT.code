<span ng-if="sqlInsert">$sql = "INSERT INTO {{firstUpper(classname)}}(<span ng-repeat="atr in atributes | filter:isNotPk">{{atr.name}}<span ng-if="!$last">,</span></span>) 
	VALUES(<span ng-repeat="atr in atributes | filter:isNotPk"><span ng-if="sqlPDO">:{{atr.name}}</span><span ng-if="!sqlPDO"><span ng-if="sqlGetSet">'".${{firstLower(classname)}}->get{{firstUpper(atr.name)}}()."'</span><span ng-if="!sqlGetSet">'{${{(atr.name)}}}'</span></span><span ng-if="!$last">,</span></span>);
